<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <TextView
        android:id="@+id/tv_order_id01"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="#D9B3FF"
        android:gravity="center_vertical"
        android:padding="5dp"
        android:text="订单编号：2014121422"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/list_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="Large Text"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:visibility="gone" />

    <TextView
        android:id="@+id/list_description"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:visibility="gone" />

    <ImageView
        android:id="@+id/list_iv_order02"
        android:layout_width="90dp"
        android:layout_height="70dp"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/tv_order_id01"
        android:layout_marginLeft="14dp"
        android:layout_marginTop="18dp"
        android:background="@drawable/iv_bg"
        android:padding="3dp"
        android:scaleType="fitXY"
        android:src="@drawable/logo12" />

    <TextView
        android:id="@+id/tv_order_name01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/list_iv_order02"
        android:layout_marginLeft="22dp"
        android:layout_toRightOf="@+id/list_iv_order02"
        android:text="商品名称：爱马仕" />

    <TextView
        android:id="@+id/tv_order_money01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/tv_order_name01"
        android:layout_below="@+id/tv_order_name01"
        android:text="订单金额：199" />

    <TextView
        android:id="@+id/tv_order_renttime01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/tv_order_money01"
        android:layout_below="@+id/tv_order_money01"
        android:text="租期：7天" />

    <TextView
        android:id="@+id/tv_order_pay01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/tv_order_renttime01"
        android:layout_below="@+id/tv_order_renttime01"
        android:text="下单时间：2014年12月04日" />

    <com.zjwy.tiaobaojinew.widget.CircleImageView
        android:id="@+id/iv_tomore"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_marginRight="20dp"
        android:clickable="true"
        android:visibility="gone" />

    <TextView
        android:id="@+id/tv_order_state01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/tv_order_id01"
        android:layout_alignBottom="@+id/tv_order_id01"
        android:layout_alignParentRight="true"
        android:layout_marginRight="20dp"
        android:text="未付款"
        android:textColor="#FF0000" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="10dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/bt_order_cancel01"
        android:text="  " />

    <Button
        android:id="@+id/bt_detail01"
        android:layout_width="40dp"
        android:layout_height="25dp"
        android:layout_alignBaseline="@+id/bt_order_cancel01"
        android:layout_alignBottom="@+id/bt_order_cancel01"
        android:layout_marginLeft="20dp"
        android:layout_toRightOf="@+id/bt_order_comit01"
        android:background="@drawable/bright_btn_bg"
        android:padding="3dp"
        android:text="详情"
        android:textSize="12dp" />

    <Button
        android:id="@+id/bt_order_comit01"
        android:layout_width="40dp"
        android:layout_height="25dp"
        android:layout_above="@+id/textView3"
        android:layout_marginLeft="20dp"
        android:layout_toRightOf="@+id/bt_order_cancel01"
        android:background="@drawable/bright_btn_bg"
        android:padding="3dp"
        android:text="评论"
        android:textSize="12dp" />

    <Button
        android:id="@+id/bt_order_cancel01"
        android:layout_width="60dp"
        android:layout_height="25dp"
        android:layout_alignLeft="@+id/tv_order_pay01"
        android:layout_below="@+id/list_iv_order02"
        android:layout_marginTop="10dp"
        android:background="@drawable/bright_btn_bg"
        android:padding="3dp"
        android:text="删除订单"
        android:textSize="12dp" />

</RelativeLayout><!-- From: file:/Users/gaoruishan/android/android_studio/DiaoBaoJiNew011/app/src/main/res/layout/list_items_myorder01.xml -->